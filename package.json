{
  "name": "react-playground",
  "version": "0.1.6",
  "description": "react-playground",
  "scripts": {
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "prestart:dev": "rimraf ./i18n",
    "dev": "npm run start:dev",
    "start:dev": "run-p -c --silent test:watch build:dev translation:watchpot translation:watchpo",
    "storybook": "cross-env NODE_ENV=development start-storybook -p 3002",
    "test": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 mocha --reporter mocha-junit-reporter",
    "test:spec": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 mocha --reporter spec",
    "test:watch": "npm run test -- --watch --reporter min",
    "test:coverage": "nyc --cache npm run test -- --reporter spec",
    "test:lint": "eslint ./src",
    "build": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.js",
    "postbuild": "npm run build:server",
    "build:server": "cross-env NODE_ENV=production START_SERVICE_MOCK=true webpack --config webpack/webpack.config.server.js ",
    "build:dev": "cross-env NODE_ENV=development node ./server/index.js",
    "translation:export": "npm run translation:extract && npm run translation:createpot",
    "translation:extract": "babel-node ./src/translation/extract.js",
    "translation:watchpot": "chokidar -p --silent '.i18n/**/*' -c 'npm run translation:createpot'",
    "translation:watchpo": "chokidar -p --silent './src/translation/po/*.po' -c 'npm run translation:import'",
    "translation:createpot": "rip json2pot ./i18n/**/*.json -o ./src/translation/po/messages.pot",
    "translation:import": "babel-node ./src/translation/po2json.js",
    "prefont:update": "rimraf $npm_package_config_fontpath && mkdirp $npm_package_config_fontpath",
    "font:update": "cd $npm_package_config_fontpath && gfdownloader Material+Icons:400 && gfdownloader Roboto:400"
  },
  "config": {
    "devhost": "0.0.0.0",
    "fontpath": "./src/app/layout/fonts"
  },
  "nyc": {
    "include": "src",
    "exclude": [
      "**/*.spec.js"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "report-dir": "./coverage",
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/drathal/react-playground.git"
  },
  "engines": {
    "node": ">=6.3.1",
    "npm": ">=3.8.3"
  },
  "dependencies": {
    "@kadira/react-storybook-addon-info": "^3.2.1",
    "autoprefixer": "^6.4.1",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-espower": "^2.3.1",
    "babel-plugin-istanbul": "^2.0.0",
    "babel-plugin-react-intl": "^2.1.4",
    "babel-plugin-system-import-transformer": "^2.3.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-promise-to-bluebird": "^1.1.0",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-plugin-transform-react-inline-elements": "^6.6.5",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-plugin-webpack-aliases": "^1.0.3",
    "babel-plugin-webpack-loaders": "^0.7.1",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-es2015-loose": "^7.0.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.11.6",
    "bluebird": "^3.4.6",
    "bootstrap": "^3.3.7",
    "chalk": "^1.1.3",
    "chokidar-cli": "^1.2.0",
    "config": "^1.21.0",
    "cookie": "^0.3.1",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.0",
    "cross-env": "^2.0.1",
    "css-loader": "^0.24.0",
    "debug": "^2.2.0",
    "deep-freeze": "0.0.1",
    "eslint-plugin-jsx-a11y": "^2.2.1",
    "exports-loader": "^0.6.3",
    "expose-loader": "^0.7.1",
    "express": "^4.13.4",
    "express-jwt": "^3.4.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.3",
    "file-loader": "^0.9.0",
    "glob": "^7.0.6",
    "google-fonts-downloader": "0.0.3",
    "html-webpack-plugin": "^2.22.0",
    "ignore-styles": "^5.0.1",
    "imagemin-webpack-plugin": "^1.0.8",
    "imports-loader": "^0.6.5",
    "inject-loader": "^2.0.1",
    "intl": "^1.2.4",
    "intl-locales-supported": "^1.0.0",
    "isomorphic-fetch": "^2.2.1",
    "json-loader": "^0.5.4",
    "jsonwebtoken": "^7.1.9",
    "lodash": "^4.15.0",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^3.1.0",
    "postcss-import": "^8.1.2",
    "postcss-loader": "^0.11.1",
    "postcss-modules-values": "^1.2.2",
    "precss": "^1.4.0",
    "pretty": "^1.0.0",
    "pretty-error": "^2.0.0",
    "pretty-format": "^3.7.0",
    "proxy-middleware": "^0.15.0",
    "react": "^15.3.1",
    "react-addons-css-transition-group": "^15.3.1",
    "react-addons-test-utils": "^15.3.1",
    "react-body-classname": "^1.0.1",
    "react-bootstrap": "^0.30.3",
    "react-dom": "^15.3.1",
    "react-gettext-parser": "^1.1.0",
    "react-helmet": "^3.1.0",
    "react-hot-loader": "^3.0.0-beta.2",
    "react-intl": "^2.1.4",
    "react-intl-loader": "^1.0.2",
    "react-intl-po": "^1.0.6",
