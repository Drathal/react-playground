{
  "name": "react-playground",
  "version": "0.1.2",
  "description": "react-playground",
  "scripts": {
    "start": "node dist/index.js",
    "prestart:dev": "rimraf ./i18n",
    "start:dev": "run-p -c --silent test:watch build:dev translation:watchpot translation:watchpo",
    "test": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 mocha --reporter mocha-junit-reporter",
    "test:watch": "chokidar -p --initial --silent './src/**/*.js' -c 'npm run test -- --reporter min'",
    "test:coverage": "nyc --cache npm run test -- --reporter spec",
    "test:lint": "eslint ./src",
    "build": "cross-env NODE_ENV=production webpack --config build/webpack.config.js",
    "postbuild": "cp ./build/server.prod.js ./dist/index.js",
    "build:dev": "cross-env NODE_ENV=development node ./build/server.dev.js",
    "translation:watchpot": "chokidar -p --silent '.i18n/**/*' -c 'npm run translation:createpot'",
    "translation:watchpo": "chokidar -p --silent './src/translation/po/*.po' -c 'npm run translation:po2json'",
    "translation:createpot": "rip json2pot ./i18n/**/*.json -o ./src/translation/po/messages.pot",
    "translation:po2json": "babel-node ./src/translation/po2json.js",
    "prefont:update": "rimraf $npm_package_config_fontpath && mkdirp $npm_package_config_fontpath",
    "font:update": "cd $npm_package_config_fontpath && gfdownloader Material+Icons:400 && gfdownloader Roboto:400"
  },
  "config": {
    "i18ncache": "./i18n"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ],
    "report-dir": "./coverage",
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/drathal/react-playground.git"
  },
  "engines": {
    "node": ">=5.11.1",
    "npm": ">=3.8.3"
  },
  "dependencies": {
    "bluebird": "^3.4.0",
    "dotenv-safe": "^2.3.1",
    "express": "^4.13.4",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.13.1",
    "react": "^15.1.0",
    "react-addons-css-transition-group": "^15.1.0",
    "react-bootstrap": "^0.29.4",
    "react-dom": "^15.1.0",
    "react-hot-loader": "^3.0.0-beta.2",
    "react-intl": "^2.1.3",
    "react-intl-redux": "0.0.10",
    "react-redux": "^4.4.5",
    "react-router": "^2.5.1",
    "react-router-redux": "^4.0.5",
    "redux": "^3.3.1",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-__coverage__": "^11.0.0",
    "babel-plugin-espower": "^2.3.1",
    "babel-plugin-react-intl": "^2.1.3",
    "babel-plugin-system-import-transformer": "^2.2.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-plugin-transform-react-inline-elements": "^6.6.5",
    "babel-plugin-webpack-loaders": "^0.7.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-webpack": "^6.4.1",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "chokidar-cli": "^1.2.0",
    "cross-env": "^1.0.8",
    "css-loader": "^0.23.1",
    "debug": "^2.2.0",
    "deep-freeze": "0.0.1",
    "dirty-chai": "^1.2.2",
    "enzyme": "^2.3.0",
    "esformatter": "^0.9.5",
    "esformatter-quotes": "^1.1.0",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.9.2",
    "eslint-plugin-jsx-a11y": "^1.4.2",
    "eslint-plugin-react": "^5.1.1",
    "exports-loader": "^0.6.3",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "google-fonts-downloader": "0.0.3",
    "html-webpack-plugin": "^2.21.0",
    "ignore-styles": "^4.0.0",
    "image-webpack-loader": "^1.7.0",
    "imports-loader": "^0.6.5",
    "inject-loader": "^2.0.1",
    "intl": "^1.2.4",
    "intl-locales-supported": "^1.0.0",
    "isparta": "^4.0.0",
    "istanbul": "^0.4.4",
    "js-beautify": "^1.6.3",
    "jsdom": "^9.2.1",
    "json-loader": "^0.5.4",
    "mkdirp": "^0.5.1",
    "mocha": "^2.5.3",
    "mocha-junit-reporter": "^1.11.1",
    "node-sass": "^3.4.2",
    "npm-run-all": "^2.2.2",
    "nyc": "^6.6.1",
    "postcss-import": "^8.1.2",
    "postcss-loader": "^0.9.1",
    "power-assert": "^1.4.1",
    "precss": "^1.4.0",
    "pretty-error": "^2.0.0",
    "proxy-middleware": "^0.15.0",
    "react-addons-test-utils": "^15.1.0",
    "react-gettext-parser": "^1.0.1",
    "react-intl-loader": "^1.0.2",
    "react-intl-po": "^1.0.5",
    "redux-mock-store": "^1.1.1",
    "sass-loader": "^4.0.0",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.1",
    "to-object-by": "^1.0.3",
    "url-loader": "^0.5.7",
    "wallaby-webpack": "0.0.22",
    "webpack": "^2.1.0-beta.14",
    "webpack-cleanup-plugin": "^0.2.0",
    "webpack-dev-server": "^2.1.0-beta.0",
    "webpack-validator": "^2.2.0"
  }
}
