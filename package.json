    "webpack-validator": "^2.2.7"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.5.1",
    "chai": "^3.5.0",
    "dirty-chai": "^1.2.2",
    "enzyme": "^2.4.1",
    "esformatter": "^0.9.6",
    "esformatter-quotes": "^1.1.0",
    "eslint": "^3.4.0",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-import-resolver-webpack": "^0.5.1",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-react": "^6.2.0",
    "isparta": "^4.0.0",
    "istanbul": "^0.4.5",
    "js-beautify": "^1.6.4",
    "jsdom": "^9.4.5",
    "mocha": "^3.0.2",
    "mocha-junit-reporter": "^1.12.0",
    "nyc": "^8.1.0",
    "power-assert": "^1.4.1",
    "sinon": "^1.17.5",
    "sinon-chai": "^2.8.0",
    "wallaby-webpack": "0.0.23"
  }
}
